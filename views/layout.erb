<!DOCTYPE HTML>
<html>
<head>

  <link rel="stylesheet" href="/styles/global.css">

  <% settings.stylesManifest.each do |filename| %>
  <link rel="stylesheet" href="/styles/<%= filename %>.css">
  <% end %>

  <script type="text/javascript" src='https://www.google.com/jsapi?autoload={"modules":[{"name":"visualization","version":"1","packages":["corechart","sankey"]}]}'></script>
  
  <script src="http://yui.yahooapis.com/3.17.2/build/yui/yui.js"></script>
  <% settings.scriptsManifest.each do |filename| %>
  <script src="/scripts/<%= filename %>.js"></script>
  <% end %>


</head>
<body>
  <script>
    google.load('visualization', '1.0', null);
  </script>

  <!-- begin content -->
  <%= yield %>
  <!-- end content -->
</body>
</html>