<script>
  google.setOnLoadCallback(function() {
    //initialize!
    YUI().use('bmp-widget-gchart', 'yui-later', 'jsb-data-util', function(Y) {
      Y.Data.get({
        url: 'http://localhost:3000/api/acs?groupby=sex&use_descriptions=1',
      }).then(function(result) {
        data = result.results;
        data.splice(0,0,result.fields);
        var chart = new Y.BMP.Widget.GChart({
          chartType: 'PieChart',
          dataTable: result.results,
          options: {
            height: 5000
          },
        });

        chart.render(Y.one('test-chart-wrapper'));
      }, function(err) {
        console.log(err);
      }); 
     
    });
  });
</script>
<div class='test-chart-wrapper'></div>